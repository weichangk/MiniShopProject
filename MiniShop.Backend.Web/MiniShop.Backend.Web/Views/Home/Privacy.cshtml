@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>



<h1>Claims Info</h1>
<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>RefreshToken</h2>
<p>@ViewData["RefreshToken"]</p>

<hr />

<h1>User Info</h1>

<h2>IdToken</h2>
<p>@ViewData["IdToken"]</p>

<h2>AccessToken</h2>
<p>@ViewData["AccessToken"]</p>

<h2>RefreshToken</h2>
<p>@ViewData["RefreshToken"]</p>

<h2>UserName</h2>
<p>@ViewData["UserName"]</p>

<h2>Rank</h2>
<p>@ViewData["Rank"]</p>

<h2>ShopId</h2>
<p>@ViewData["ShopId"]</p>

<h2>StoreId</h2>
<p>@ViewData["StoreId"]</p>

<h2>PhoneNumber</h2>
<p>@ViewData["PhoneNumber"]</p>

<h2>Email</h2>
<p>@ViewData["Email"]</p>

<h2>IsFreeze</h2>
<p>@ViewData["IsFreeze"]</p>

<h2>CreatedTime</h2>
<p>@ViewData["CreatedTime"]</p>

