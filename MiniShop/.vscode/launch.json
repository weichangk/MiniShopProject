{
    "version": "0.2.0",
    "configurations": [
    {
        "name": ".NET Core Launch (MiniShop.Mvc)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/MiniShop.Mvc/bin/Debug/netcoreapp3.1/MiniShop.Mvc.dll",
        "args": [],
        "cwd": "${workspaceFolder}/MiniShop.Mvc",
        "stopAtEntry": false,
        "serverReadyAction": {
            "action": "openExternally",
            "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
            "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
            "/Views": "${workspaceFolder}/Views"
        }
    },
    {
        "name": ".NET Core Launch (MiniShop.Api)",
        "type": "coreclr",
